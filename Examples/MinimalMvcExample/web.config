<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<configSections>
		<sectionGroup name="isis.web">
			<section name="embeddedResources" type="Isis.Web.Configuration.EmbeddedResourcesSection, Isis" />
		</sectionGroup>
		<sectionGroup name="zeus" type="Zeus.Configuration.ZeusSectionGroup, Zeus">
			<section name="admin" type="Zeus.Configuration.AdminSection, Zeus" requirePermission="false" />
			<section name="database" type="Zeus.Configuration.DatabaseSection, Zeus" requirePermission="false" />
			<section name="globalization" type="Zeus.Configuration.GlobalizationSection, Zeus" requirePermission="false" />
			<section name="host" type="Zeus.Configuration.HostSection, Zeus" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<connectionStrings>
		<add name="Zeus" connectionString="Server=.\SQLEXPRESS;Database=ZeusMinimalMvcExample;Integrated Security=True" />
	</connectionStrings>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="NHibernate" publicKeyToken="AA95F207798DFDB4" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-2.0.1.3999" newVersion="2.0.1.4000" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
	<system.net>
		<mailSettings>
			<smtp>
				<network host="localhost" />
			</smtp>
		</mailSettings>
	</system.net>
	<system.web>
		<authentication mode="None" />
		<compilation debug="true">
			<assemblies>
				<add assembly="System.Core, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
				<add assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add assembly="System.Web.Abstractions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add assembly="System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
				<add assembly="System.Web.Routing, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add assembly="System.Data.DataSetExtensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
				<add assembly="System.Xml.Linq, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
				<add assembly="System.Data.Linq, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
			</assemblies>
		</compilation>
		<customErrors mode="Off" />
		<globalization culture="en-GB" />
		<httpRuntime maxRequestLength="10240" />
		<pages validateRequest="false" pageParserFilterType="System.Web.Mvc.ViewTypeParserFilter, System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" pageBaseType="System.Web.Mvc.ViewPage, System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" userControlBaseType="System.Web.Mvc.ViewUserControl, System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35">
			<controls>
				<add tagPrefix="asp" namespace="System.Web.UI" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add tagPrefix="asp" namespace="System.Web.UI.WebControls" assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add tagPrefix="isis" namespace="Isis.Web.UI.WebControls" assembly="Isis" />
				<add tagPrefix="mvc" namespace="System.Web.Mvc" assembly="System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
				<add tagPrefix="zeus" namespace="Zeus.Web.UI.WebControls" assembly="Zeus" />
			</controls>
			<namespaces>
				<add namespace="System.Linq" />
				<add namespace="System.Web.Mvc" />
				<add namespace="System.Web.Mvc.Html" />
				<add namespace="System.Web.Routing" />
			</namespaces>
		</pages>
		<sessionState mode="InProc" />
	</system.web>
	<system.codedom>
		<compilers>
			<compiler language="c#;cs;csharp" extension=".cs" type="Microsoft.CSharp.CSharpCodeProvider,System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" warningLevel="4">
				<providerOption name="CompilerVersion" value="v3.5" />
				<providerOption name="WarnAsError" value="false" />
			</compiler>
		</compilers>
	</system.codedom>
	<isis.web>
		<embeddedResources>
			<virtualPathAssemblies>
				<add assembly="Zeus.Admin" path="admin" />
			</virtualPathAssemblies>
			<webResourceAssemblies>
				<add assembly="Zeus" path="webResources/zeus" />
				<add assembly="Zeus.Admin" path="webResources/zeus/admin" />
			</webResourceAssemblies>
		</embeddedResources>
	</isis.web>
	<zeus>
		<admin name="Minimal Example" path="admin">
   <installer checkInstallationStatus="true" mode="Normal" />
   <recycleBin enabled="true" />
   <tree tooltipsEnabled="false" />
  </admin>
		<database connectionStringName="Zeus" cacheEnabled="true" cacheProviderClass="NHibernate.Caches.SysCache2.SysCacheProvider, NHibernate.Caches.SysCache2" />
		<globalization enabled="true" useBrowserLanguagePreferences="false" />
		<host rootItemID="1">
   <sites>
    <clear />
    <site id="DefaultSite" startPageID="4" description="Default Site">
     <siteHosts>
      <add name="*" />
     </siteHosts>
    </site>
   </sites>
   <web extension="" rewrite="None" />
  </host>
	</zeus>
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false" />
		<handlers>
			<remove name="WebServiceHandlerFactory-Integrated" />
			<remove name="ScriptHandlerFactory" />
			<remove name="ScriptHandlerFactoryAppServices" />
			<remove name="ScriptResource" />
			<add name="MVC" path="*" verb="*" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework\v2.0.50727\aspnet_isapi.dll" resourceType="Unspecified" requireAccess="None" preCondition="classicMode,runtimeVersionv2.0,bitness32" />
			<add name="ScriptHandlerFactory" verb="*" path="*.asmx" preCondition="integratedMode" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="ScriptHandlerFactoryAppServices" verb="*" path="*_AppService.axd" preCondition="integratedMode" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="ScriptResource" preCondition="integratedMode" verb="GET,HEAD" path="ScriptResource.axd" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="MvcHttpHandler" preCondition="integratedMode" verb="*" path="*.mvc" type="System.Web.Mvc.MvcHttpHandler, System.Web.Mvc, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="UrlRoutingHandler" preCondition="integratedMode" verb="*" path="UrlRouting.axd" type="System.Web.HttpForbiddenHandler, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
			<add name="FileHandler" preCondition="integratedMode" verb="GET" path="File.axd" type="Zeus.Web.Handlers.FileHandler, Zeus" />
			<add name="FileDataHandler" preCondition="integratedMode" verb="GET" path="FileData.axd" type="Zeus.Web.Handlers.FileDataHandler, Zeus" />
			<add name="ChunkedFileUploadHandler" preCondition="integratedMode" verb="POST" path="ChunkedFileUpload.axd" type="Zeus.Web.Handlers.ChunkedFileUploadHandler, Zeus" />
			<add name="PostedFileUploadHandler" preCondition="integratedMode" verb="POST" path="PostedFileUpload.axd" type="Zeus.Web.Handlers.PostedFileUploadHandler, Zeus" />
		</handlers>
		<modules runAllManagedModulesForAllRequests="true">
			<remove name="ScriptModule" />
			<remove name="UrlRoutingModule" />
			<add name="ScriptModule" preCondition="managedHandler" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web.Routing, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
			<add name="AuthenticationModule" type="Isis.Web.Security.AuthenticationModule, Isis.FrameworkBlocks.WebSecurity" />
			<add name="AuthorizationModule" type="Isis.Web.Security.AuthorizationModule, Isis.FrameworkBlocks.WebSecurity" />
			<add name="EmbeddedWebResourceModule" type="Isis.Web.UI.EmbeddedWebResourceModule, Isis" />
			<add name="DynamicImageModule" type="SoundInTheory.DynamicImage.DynamicImageModule, SoundInTheory.DynamicImage" />
		</modules>
	</system.webServer>
</configuration>