<viewdata model="Zeus.AddIns.Blogs.Mvc.ViewModels.CategoryListWidgetViewModel"/>

<h3 class="first">Categories</h3>

<ul class="articles">
	<li each="var categoryEntry in Model.CategoryEntries">
		<a href="${categoryEntry.Category.Url}">${categoryEntry.Category.Title} (${categoryEntry.Count})</a>
	</li>
</ul>