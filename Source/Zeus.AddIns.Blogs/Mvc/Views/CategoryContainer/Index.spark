<viewdata model="Zeus.AddIns.Blogs.Mvc.ViewModels.CategoryContainerViewModel"/>

<content name="head">
	<link rel="stylesheet" type="text/css" href="/assets/blogs/css/core.css" />
</content>

<ul>
	<li each="var category in Model.Categories">
		<a href="${category.Url}">${category.Title}</a>
	</li>
</ul>